<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <ImageButton
        android:id="@+id/image"
        android:layout_width="400dp"
        android:layout_height="163dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/boximg2"
        android:contentDescription="@string/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/first"
        tools:ignore="ImageContrastCheck" />

    <Button
        android:id="@+id/add"
        android:layout_width="98dp"
        android:layout_height="48dp"
        android:layout_marginStart="255dp"
        android:layout_marginTop="165dp"
        android:width="60dp"
        android:backgroundTint="#C8A695"
        android:text="@string/add12"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/more"
        android:layout_width="98dp"
        android:layout_height="48dp"
        android:layout_marginStart="255dp"
        android:layout_marginTop="210dp"
        android:width="60dp"
        android:backgroundTint="#C8A695"
        android:text="@string/more12"
        tools:ignore="TextContrastCheck" />


    <TextView
        android:id="@+id/sername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="170dp"
        android:text="@string/sername"
        android:textAlignment="viewStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/serdes"
        android:layout_width="206dp"
        android:layout_height="72dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="190dp"
        android:text="@string/serdes"
        android:textAlignment="viewStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="italic"
        tools:ignore="TextSizeCheck" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="360dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="11dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_search"
        android:drawableStart="@drawable/customsearchicon"
        android:drawablePadding="12sp"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:text="@string/mani"
        android:textColor="#090606"
        android:textSize="14sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill,LabelFor" />

    <Button
        android:id="@+id/sort"
        android:layout_width="56dp"
        android:layout_height="48dp"
        android:layout_marginStart="330dp"
        android:layout_marginTop="70dp"
        android:backgroundTint="#C8A695"
        android:drawableTop="@drawable/ic_sort"
        android:textColor="#EDE3E3"
        android:textColorHighlight="#F1EAEA"
        android:textColorLink="#0FFFFFFF"
        android:contentDescription="@string/forsorting"/>

    <TextView
        android:id="@+id/MYres"
        android:layout_width="345dp"
        android:layout_height="35dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="24dp"
        android:layout_marginRight="10dp"
        android:lineSpacingExtra="-1sp"
        android:text="@string/home1"
        android:textColor="#895D47"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextSizeCheck" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="1dp"
        app:menu="@menu/menu" />

</RelativeLayout>
